{
  "name": "regen-eliza",
  "version": "1.0.0",
  "description": "ReF[AI] Agent Ecosystem - Specialized AI agents for Regenerative Finance built on ElizaOS",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "refi-dao-community-agent",
    "refi-dao-global-agent", 
    "refi-local-node-agent",
    "my-agent"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "clean": "turbo clean"
  },
  "keywords": [
    "ai",
    "agents",
    "refi",
    "regenerative-finance",
    "sustainability",
    "blockchain",
    "dao",
    "elizaos"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ReFiDAO/regen_eliza-refi_dao.git"
  },
  "bugs": {
    "url": "https://github.com/ReFiDAO/regen_eliza-refi_dao/issues"
  },
  "homepage": "https://github.com/ReFiDAO/regen_eliza-refi_dao#readme",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.1.44",
  "devDependencies": {
    "@types/node": "^22.5.4",
    "prettier": "^3.0.0",
    "turbo": "^2.2.3",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@elizaos/cli": "workspace:*",
    "@elizaos/core": "workspace:*"
  }
}